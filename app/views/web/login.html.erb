<% @page_title = "Paste Bin | Login" -%>
<div class= "Login_Form">
	<h1>Paste Bin - Login</h1>
	<%= form_tag(:controller => 'users', :action => 'login_attempt') do %>
		<p>Username: <%= text_field_tag(:username) %></p> 
		<p>Password: <%= password_field_tag :password %></p> 
	 	<%= submit_tag("Log In") %>
	<% end %>
</div>
<%= link_to "Back to home page", web_index_path %>